<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wms.mapper.systemsetting.UserMangerMapper">

	<select id="sel" resultType="com.wms.model.UserInfo">

		SELECT
		id,
		username,
		usercode,
		PASSWORD,
		unit_code,
		LEVEL,
		user_state,
		tel
		FROM
		userinfo
		WHERE
		1 = 1
		<if test="db_name!=null and db_name!=''">
			and username = #{username}
		</if>
		<if test="db_name!=null and db_name!=''">
			and usercode like '${usercode}%'
		</if>
	</select>

</mapper>
package com.wms.controller.systemsetting;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.bind.annotation.ResponseBody;

import com.wms.common.BackMessage;
import com.wms.service.storemanagement.BackOrTakeStoreService;
import com.wms.service.systemsetting.UserMangerService;

//人员管理
@Controller
@SuppressWarnings("all")
@RequestMapping("BackOrTakeStoreController")
public class UserMangerController {

	@Autowired
	private UserMangerService service;
	
	//退料
	@RequestMapping(value = "/sel")
	@ResponseBody
	public BackMessage sel(HttpServletRequest req, HttpServletResponse res) {

		BackMessage backMessage = service.sel(req, res);
		return backMessage;
	}
	
}

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wms.mapper.query.InOutRecordsMapper">
<!-- 加载表 -->
<select id="GetInOutRecords" resultType="com.wms.model.InOutRecordsModel">
 SELECT id, goods_code,goods_num,supply_code,user_code,time,sum_money,single_money,state
FROM buy_return 
WHERE 1=1
	
	
	
<if test="startTime!=null and startTime!=''">

and time &gt; #{startTime}
</if>
<if test="endTime!=null and endTime!=''">

and time &lt; #{endTime}
</if> 
</select>
</mapper>